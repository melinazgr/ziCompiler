TERM	EQU 19	the terminal
SYM	EQU 3000
PRINT	EQU 2
	ORIG PRINT+24
BEGIN	NOP
	ENTA 0
	STA SYM+0
	ENTA 0
	STA SYM+1
	ENTA 5
	STA SYM+1
	LDA SYM+1
	STA SYM+0
L9	LDA SYM+0
	CMPA 10
	JL L8
	ENTA 0
	STA SYM+1
	LDA SYM+0
	ADD 1
	JOV TOOLARGE
	STA SYM-1
	LDA SYM-1
	STA SYM+0
L8	ENTA 120
	STA SYM+0
	HLT
TOOLARGE	OUT OVERFLOW(TERM)
	HLT 0
OVERFLOW	ALF OVER 
	ALF FLOW 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
ZERO	OUT DIVZERO(TERM)
	HLT 0
DIVZERO	ALF DIV  
	ALF ZERO 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	END BEGIN
