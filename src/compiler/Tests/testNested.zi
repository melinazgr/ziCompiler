mainclass testNested {
     public static void main ( ) {
        
        int a, b;

        a = 90;
        b = 20;

        if( a < b){
            println(a);
        }

        else {
           if(a < 100){
               println(a);
           }

           else{
               a = a + 1;
               //expected value 91
               println(a);
           }
        }
     
        a = 3;
        b = 10;

        if( a > b){
            println(a);
        }

        else{ 
           if(a == 101){
               println(a);
           }

           else{
               a = a + 1;
               //expected value 4
               println(a);
           }
        }
     }
 }