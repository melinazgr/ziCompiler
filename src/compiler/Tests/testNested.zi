mainclass testNested {
     public static void main ( ) {
        
        int a, b, c;


        a = 90;
        b = 20;
        
        c = a + b;

        if( a < b){
            int k;
            a = b;
            a = b + k * c;
            k = 0;
        }

        else {
            int l;
            b = 40;
            a = l * b;
        }

        while(a > 0){
            int k;
            a = b + k * c;
        }
     }
 }