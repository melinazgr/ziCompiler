TERM	EQU 19	the terminal
SYM	EQU 3000
PRINT	EQU 2
	ORIG PRINT+24
BEGIN	NOP
	ENTA 0
	STA SYM+0
	ENTA 0
	STA SYM+1
	ENTA 10
	STA SYM+0
	ENTA 20
	STA SYM+1
	LDA SYM+0
	CMPA SYM+1
	JL L10
	LDA SYM+1
	STA SYM+0
L10	LDA SYM+0
	CMPA SYM+1
	JE L15
	LDA SYM+1
	STA SYM+0
L15	LDA SYM+0
	CMPA SYM+1
	JL L20
	LDA SYM+1
	STA SYM+0
L20	HLT
TOOLARGE	OUT OVERFLOW(TERM)
	HLT 0
OVERFLOW	ALF OVER 
	ALF FLOW 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
ZERO	OUT DIVZERO(TERM)
	HLT 0
DIVZERO	ALF DIV  
	ALF ZERO 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	END BEGIN
