TERM	EQU 19	the terminal
SYM	EQU 3000
PRINT	EQU 2
	ORIG PRINT+24
BEGIN	NOP
	ENTA 0
	STA SYM+0
	ENTA 0
	STA SYM+1
	ENTA 0
	STA SYM+2
	LDA SYM+1
	ADD SYM+2
	JOV TOOLARGE
	STA SYM-1
	LDA SYM-1
	STA SYM+0
	LDA SYM+1
	SUB SYM+2
	JOV TOOLARGE
	STA SYM-1
	LDA SYM-1
	STA SYM+0
	LDA SYM+1
	MUL SYM+2
	JANZ TOOLARGE
	STX SYM-1
	LDA SYM-1
	STA SYM+0
	LDA SYM+2
	JAZ ZERO
	LDA SYM+1
	SRAX 5
	DIV SYM+2
	JOV TOOLARGE
	STA SYM-1
	LDA SYM-1
	STA SYM+0
	HLT
TOOLARGE	OUT OVERFLOW(TERM)
	HLT 0
OVERFLOW	ALF OVER 
	ALF FLOW 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
ZERO	OUT DIVZERO(TERM)
	HLT 0
DIVZERO	ALF DIV  
	ALF ZERO 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	END BEGIN
