TERM	EQU 19	the terminal
SYM	EQU 3000
PRINT	EQU 2
	ORIG PRINT+24
BEGIN	NOP
	ENTA 0
	STA SYM+0
	ENTA 0
	STA SYM+1
	ENTA 0
	STA SYM+2
	ENTA 90
	STA SYM+0
	ENTA 20
	STA SYM+1
	LDA SYM+0
	ADD SYM+1
	JOV TOOLARGE
	STA SYM-1
	LDA SYM-1
	STA SYM+2
	LDA SYM+0
	CMPA SYM+1
	JL L14
	ENTA 0
	STA SYM+3
	LDA SYM+1
	STA SYM+0
	LDA SYM+3
	MUL SYM+2
	JANZ TOOLARGE
	STX SYM-2
	LDA SYM+1
	ADD SYM-2
	JOV TOOLARGE
	STA SYM-3
	LDA SYM-3
	STA SYM+0
	ENTA 0
	STA SYM+3
L14	ENTA 0
	STA SYM+3
	ENTA 40
	STA SYM+1
	LDA SYM+3
	MUL SYM+1
	JANZ TOOLARGE
	STX SYM-2
	LDA SYM-2
	STA SYM+0
	ENTA 0
	STA SYM+3
L33	LDA SYM+0
	CMPA 0
	JG L30
	LDA SYM+3
	MUL SYM+2
	JANZ TOOLARGE
	STX SYM-2
	LDA SYM+1
	ADD SYM-2
	JOV TOOLARGE
	STA SYM-3
	LDA SYM-3
	STA SYM+0
L30	HLT
TOOLARGE	OUT OVERFLOW(TERM)
	HLT 0
OVERFLOW	ALF OVER 
	ALF FLOW 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
ZERO	OUT DIVZERO(TERM)
	HLT 0
DIVZERO	ALF DIV  
	ALF ZERO 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	END BEGIN
