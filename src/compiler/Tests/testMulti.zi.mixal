TERM	EQU 19	the terminal
SYM	EQU 3000
PRINT	EQU 2
	ORIG PRINT+24
BEGIN	NOP
	ENTA 0
	STA SYM+0
	ENTA 0
	STA SYM+1
	ENTA 0
	STA SYM+2
	ENTA 50
	STA SYM+0
	ENTA 80
	STA SYM+1
	LDA SYM+0
	MUL SYM+1
	JANZ TOOLARGE
	STX SYM-1
	LDA SYM-1
	STA SYM+2
	LDA SYM+2
	CHAR
	STA PRINT
	STX PRINT+1
	OUT PRINT(TERM)
	HLT
TOOLARGE	OUT OVERFLOW(TERM)
	HLT 0
OVERFLOW	ALF OVER 
	ALF FLOW 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
ZERO	OUT DIVZERO(TERM)
	HLT 0
DIVZERO	ALF DIV  
	ALF ZERO 
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	ALF      
	END BEGIN
